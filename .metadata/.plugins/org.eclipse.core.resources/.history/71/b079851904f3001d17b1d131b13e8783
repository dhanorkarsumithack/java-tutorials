package com.sumit.Linkedlist;

import java.util.LinkedList;

public class Linkedlist {

	public static void main(String[] args) {
		
		//declare 
		LinkedList  l=new LinkedList();
		
		l.add("sumit");
		l.add(123);
		l.add('A');
		l.add("ram");
		l.add(null);
		
		System.out.println(l);
		
		System.out.println(l.size());
		
	}
}
